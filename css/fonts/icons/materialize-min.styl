@font-face {
  font-family: 'fontello';
  src: url('materialize-min/fontello.eot?14444094');
  src: url('materialize-min/fontello.eot?14444094#iefix') format('embedded-opentype'),
       url('materialize-min/fontello.woff2?14444094') format('woff2'),
       url('materialize-min/fontello.woff?14444094') format('woff'),
       url('materialize-min/fontello.ttf?14444094') format('truetype'),
       url('materialize-min/fontello.svg?14444094#fontello') format('svg');
  font-weight: normal;
  font-style: normal;
}
